#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int A[n];
    for(int i=0;i<n;i++)
    cin>>A[i];
    int min=INT_MAX,max=INT_MIN;
    for(int i=0;i<n;i++)
    {
        if(A[i]>max)
            max=A[i];
        if(A[i]<min)
            min=A[i];
    }
    cout<<max<<endl;
    cout<<min;
    return 0;
}
